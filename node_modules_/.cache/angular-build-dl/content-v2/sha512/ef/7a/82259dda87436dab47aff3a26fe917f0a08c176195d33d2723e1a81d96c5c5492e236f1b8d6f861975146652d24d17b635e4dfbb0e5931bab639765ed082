let JobDetailPage = class JobDetailPage {
    constructor(api, loadingController, route, router, callNumber, storage) {
        this.api = api;
        this.loadingController = loadingController;
        this.route = route;
        this.router = router;
        this.callNumber = callNumber;
        this.storage = storage;
        this.detail = {};
