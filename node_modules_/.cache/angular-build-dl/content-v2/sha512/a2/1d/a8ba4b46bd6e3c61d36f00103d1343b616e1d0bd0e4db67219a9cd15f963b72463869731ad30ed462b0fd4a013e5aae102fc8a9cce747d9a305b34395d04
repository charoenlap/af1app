>\\n    </ion-refresher-content>\\n  </ion-refresher>\\n\\t<!-- <ion-searchbar ></ion-searchbar> -->\\n    <ion-list lines=\\\"full\\\">\\n      <ion-item  *ngFor=\\\"let job of jobs\\\" routerLink=\\\"/members/job-detail/{{job.id_booking}}\\\">\\n      \\t<ion-icon slot=\\\"start\\\" color=\\\"success\\\" name=\\\"mail\\\"></ion-icon>\\n      \\t<ion-grid>\\n\\t\\t  \\t<ion-row>\\n\\t        \\t<ion-col size=\\\"12\\\">\\n\\t\\t\\t\\t\\t<ion-label><b>{{job.key}} {{job.person}}</b></ion-label>\\n\\t        \\t</ion-col>\\n\\t        \\t<ion-col size=\\\"12\\\">\\n\\t\\t\\t\\t\\t<ion-label>{{job.customer_name}}</ion-label>\\n\\t        \\t</ion-col>\\n\\t        \\t<ion-col size=\\\"12\\\">\\n\\t        \\t\\t<ion-label>{{job.district}} {{job.province}} {{job.postcode}}</ion-label>\\n\\t        \\t</ion-col>\\n\\t        \\t<ion-col size=\\\"12\\\">\\n\\t\\t\\t\\t\\t<ion-label><ion-badge color=\\\"dark\\\">{{job.cod}}</ion-badge> เวลารับสินค้า {{job.get_datetime}}</ion-label>\\n\\t        \\t</ion-col>\\n\\t        </ion-row>\\n\\t\\t</ion-grid>\\n      </ion-item>\\n    </ion-list>\\n</ion-content>\\n\";","export default \"ion-content {\\n  --ion-background-color: #fbfbfb;\\n}\\n\\nion-list {\\n  --ion-background-color: #fff;\\n}\\n\\nion-label {\\n  font-size: 0.8rem !important;\\n}\\n\\n.inprogress {\\n  display: block;\\n  padding: 13px 20px;\\n  text-align: center;\\n  background: #28a745;\\n  border-radius: 50%;\\n  font-size: 1.1rem;\\n  color: #fff;\\n  width: 50px;\\n  height: 50px;\\n}\\n/*# sourceMappingURL=data:application/json;base64, */\";","import { IonicModule } from '@ionic/angular';\nimport { RouterModule } from '@angular/router';\nimport { NgModule } from '@angular/core';\nimport { CommonModule } from '@angular/common';\nimport { FormsModule } from '@angular/forms';\nimport { jobPage } from './job.page';\n\n@NgModule({\n  imports: [\n    IonicModule,\n    CommonModule,\n    FormsModule,\n    RouterModule.forChild([{ path: '', component: jobPage }])